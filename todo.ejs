  <section id="carousel-part">
    <div id="todolistCarousel" class="carousel slide" data-ride="carousel">
      <ol class="carousel-indicators">
        <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
      </ol>
      <div class="carousel-inner">
        <div class="carousel-item active">
          <div class="carousel-insider">
              <div class="car-font">
                <h1 class="n1">Set your priorities</h1><br>
                <h1 class="n2">and Goals.</h1>
                <img id="text-img" src="images/car-icon2.png" alt="">
                <img id="text-img2" src="images/car-icon2.png" alt="">
                <img id="text-img3" src="images/car-icon2.png" alt="">
              </div>

                <img src="images/car- (2).jpg"></img>
          </div>
        </div>
        <div class="carousel-item">
          <div class="carousel-insider">
            <div class="car-font">
              <h1 class="n1">You'll forgot </h1><br>
              <h1 class="n2">but list won't.</h1>
              <img id="text-img" src="images/car-icon2.png" alt="">
              <img id="text-img2" src="images/car-icon2.png" alt="">
              <img id="text-img3" src="images/car-icon2.png" alt="">
            </div>

              <img src="images/car- (3).jpg"></img>
          </div>
        </div>
        <div class="carousel-item">
          <div class="carousel-insider">
              <img src="images/car- (4).jpg"></img>
              <img class="car-last-img" src="images/car- (6).jpg"></img>

          </div>
        </div>
      </div>
      <a class="carousel-control-prev" href="#todolistCarousel" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="carousel-control-next" href="#todolistCarousel" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
  </section>

  <section id="todolist-calendar">
    <div class="todolist-part">
      <div class="box" id="heading">
    <h1> <%= listTitle %> </h1>
  </div>

  <div class="box">
   <%newListItems.forEach(function(elements){%>
     <form action="/delete" method="post">
       <div class="item">
         <input type="checkbox" name="checkbox" value=<%=elements.id %> onChange="this.form.submit()">
         <p> <%=elements.name%> </p>
       </div>
       <input type="hidden" name="listName" value="<%= listTitle %>"></input>
     </form>

      <% }) %>

      <form class="item" action="/" method="post">
        <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
        <button type="submit" name="list" value="<%= listTitle %>" >+</button>
      </form>
  </div>
    </div>

  </section>

